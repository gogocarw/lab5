import java.util.ArrayList;

public class Data {
    // Encapsulate the list so only this class manages it
    private ArrayList<String> list = new ArrayList<>();

    public void addBack(String text) {
        list.add(text);
    }

    public void addAtIndex(int index, String text) {
        list.add(index, text);
    }

    public void edit(int index, String text) {
        list.set(index, text);
    }

    public void removeByIndex(int index) {
        list.remove(index);
    }

    public void removeByValue(String text) {
        list.remove(text);
    }

    public void display() {
        System.out.print("Data =");
        for (String s : list) {
            System.out.print(" " + s);
        }
        System.out.println();
    }
}
